type Query {
  variant(
    variantId: String
    rsid: String
    vrsId: String
    dataset: DatasetId!
  ): VariantDetails
}

enum DatasetId {
  gnomad_r4
  gnomad_r4_non_ukb
  gnomad_r2_1
}

enum ReferenceGenomeId {
  GRCh37
  GRCh38
}

type VariantDetails {
  variant_id: String!
  reference_genome: ReferenceGenomeId!
}